<configuration>
	<typeAliases>



		<typeAlias alias="SendNode" type="com.pojo.SendNode" />


		<typeAlias alias="User" type="com.pojo.User" />


	    <typeAlias alias="Route" type="com.pojo.Route" />


         <typeAlias alias="TotalRoute" type="com.pojo.TotalRoute" />
		

